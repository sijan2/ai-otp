# OpenAI API Key - Get from https://platform.openai.com/account/api-keys
EXTENSION_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# OAuth Settings - Get from Google Cloud Console
EXTENSION_PUBLIC_GMAIL_CLIENT_ID=your_gmail_client_id_here

# WebSocket URL - Your WebSocket server endpoint
EXTENSION_PUBLIC_WEBSOCKET_URL=your_websocket_url_here

# OAuth Redirect URI - Chrome extension redirect URI
EXTENSION_PUBLIC_REDIRECT_URI=your_extension_redirect_uri_here

# API Endpoints - Your authentication backend endpoints
EXTENSION_PUBLIC_TOKEN_EXCHANGE_URL=your_token_exchange_url_here
EXTENSION_PUBLIC_REFRESH_URL=your_refresh_url_here